{
  "name": "yourday-client",
  "version": "0.0.0",
  "scripts": {
    "start:local": "ng serve --proxy-config proxy/local.json --port 4242",
    "start:prod": "ng serve --proxy-config proxy/prod.json --port 4242",
    "start:ssl": "ng serve --port 443 --disable-host-check --host yourday.me --ssl true --ssl-key ssl/key.pem --ssl-cert ssl/certificate.pem --proxy-config proxy/local.json",
    "build:dev": "rimraf dist && ng build --configuration development",
    "build:prod": "rimraf dist && ng build --configuration production",
    "build:mobile": "rimraf ../yourday-native/www && ng build --base-href ./ --configuration mobile --output-path ../yourday-native/www",
    "artifact": "rm -f artifact.zip && cd dist/yourday-client/ && zip -r artifact.zip ./ && mv artifact.zip ../../",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "rpls": "./node_modules/rpls/bin/cli.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "15.2.5",
    "@angular/cdk": "15.2.5",
    "@angular/common": "15.2.5",
    "@angular/compiler": "15.2.5",
    "@angular/core": "15.2.5",
    "@angular/forms": "15.2.5",
    "@angular/material": "15.2.5",
    "@angular/platform-browser": "15.2.5",
    "@angular/platform-browser-dynamic": "15.2.5",
    "@angular/router": "15.2.5",
    "@angular/service-worker": "15.2.5",
    "@ngxs/logger-plugin": "^3.8.0",
    "@ngxs/storage-plugin": "^3.8.0",
    "@ngxs/store": "^3.8.0",
    "exceljs": "^4.3.0",
    "file-saver": "^2.0.5",
    "hammerjs": "^2.0.8",
    "moment": "^2.29.4",
    "ngx-mask": "^15.1.0",
    "ngx-material-timepicker": "^5.5.3",
    "ngx-socket-io": "^4.4.0",
    "platform": "^1.3.6",
    "rxjs": "~7.8.0",
    "tslib": "^2.5.0",
    "web-push": "^3.5.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "15.2.4",
    "@angular-eslint/builder": "15.2.1",
    "@angular-eslint/eslint-plugin": "15.2.1",
    "@angular-eslint/eslint-plugin-template": "15.2.1",
    "@angular-eslint/schematics": "15.2.1",
    "@angular-eslint/template-parser": "15.2.1",
    "@angular/cli": "^15.2.4",
    "@angular/compiler-cli": "15.2.5",
    "@types/cordova": "^0.0.34",
    "@types/file-saver": "^2.0.5",
    "@types/hammerjs": "^2.0.41",
    "@types/jasmine": "~4.3.1",
    "@types/platform": "^1.3.4",
    "@types/web-push": "^3.3.2",
    "@typescript-eslint/eslint-plugin": "5.57.1",
    "@typescript-eslint/parser": "5.57.1",
    "eslint": "^8.37.0",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.1",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "rpls": "^1.0.16",
    "typescript": "~4.9.5"
  }
}
